{
    "platform": {
      "apiRuntime": "node:20"
    },
    "routes": [
      {
        "route": "/favicon.ico"
      },
      {
        "route": "/build/*"
      },
      {
        "route": "/*",
        "rewrite": "/api/ssr"
      }
